<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin DashBoard | Househunt</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.8.2/css/all.css">
    <link rel="stylesheet" type="text/css" media="screen" href="/public/static/styles/main.css"/>
</head>
<body>
    <nav>
        <div class="brand">
            House Hunt Admin
        </div>
        <div class="nav-items">
            <ul>
                <li>Logout (Edwin)</li>
            </ul>
        </div>
    </nav>
    <select id="filter">
        <option>
            Sort By
        </option>
    </select>
    <table>
        <tr>
            <th>Title</th>
            <th>Location</th>
            <th>Beds</th>
            <th>Baths</th>
            <th> </th>
            <th></th>
        </tr>
        <%listings.map((listing)=>{%>
            <tr>
                <td><%=listing.title%></td>
                <td><%=listing.location%></td>
                <td><%=listing.beds%></td>
                <td><%=listing.baths%></td>
                <td><a href="listings/edit/<%=listing._id%>"><i class="far fa-edit"></i></a></td>
                <td><a href="listings/delete/<%=listing._id%>"><i class="fas fa-trash"></i></a></td>
            </tr>
        <%})%>
    </table>
</body>
</html>