<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin DashBoard | Househunt</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.8.2/css/all.css">
    <link rel="stylesheet" type="text/css" media="screen" href="/public/static/styles/main.css"/>
</head>
<body>
    <nav>
        <div class="brand">
            House Hunt Admin
        </div>
        <div class="nav-items">
            <ul>
                <li>Logout (Edwin)</li>
            </ul>
        </div>
    </nav>
    <select id="filter">
        <option>Sort</option>
        <option value="">
            All
        </option>
        <option value="1">1 Bedroom</option>
        <option value="2">2 Bedroom</option>
        <option value="3">3 Bedroom</option>
        <option value="4">4 Bedroom</option>
    </select>
    <p id="result-count">Showing <%=listings.length%> Result(s)</p>
    <table>
        <tr>
            <th>Title</th>
            <th>Location</th>
            <th>Beds</th>
            <th> </th>
            <th></th>
        </tr>
        <%listings.map((listing)=>{%>
            <tr>
                <td><%=listing.title%></td>
                <td><%=listing.location%></td>
                <td><%=listing.beds%></td>
                <td><a href="listings/edit/<%=listing._id%>"><i class="far fa-edit"></i></a></td>
                <td><a href="listings/delete/<%=listing._id%>"><i class="fas fa-trash"></i></a></td>
            </tr>
        <%})%>
    </table>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="/public/static/scripts/app.js"></script>
</body>
</html>